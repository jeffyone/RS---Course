    "import math\n",
    "import numpy as np\n",
    "def distance(x1,y1,x2,y2):\n",
    "    x=x1-x2\n",
    "    y=y1-y2\n",
    "    d=x**2+y**2\n",
    "    dis=math.sqrt(d)\n",
    "    print(dis)"
